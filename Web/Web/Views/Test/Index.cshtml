@using System.Globalization
@{
    foreach(var element in Context.Request.Cookies.AllKeys)
    {
        @(element) 
        @Html.Raw("=>")
        var httpCookie = Context.Request.Cookies[element];
        if (httpCookie != null)
        {
            @httpCookie.Value.ToString(CultureInfo.InvariantCulture)
        }
        @Html.Raw("\n")
    }
}